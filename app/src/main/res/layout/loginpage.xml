<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/some_id"
        android:layout_width="127dp"
        android:layout_height="34dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/some_id"
        android:textAppearance="@style/some_id"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="23dp" />

    <TextView
        android:id="@+id/magic_shop"
        android:layout_width="140dp"
        android:layout_height="37dp"
        android:layout_alignParentLeft="true"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/magic_shop_1"
        android:textAppearance="@style/magic_shop_1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="678dp" />

    <android.widget.Button
        android:id="@+id/login_button2"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/loginbutton"
        android:text="로그인"
        android:textSize="18sp"
        android:textColor="#FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="239dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="32dp"
        android:layout_height="26dp"
        app:srcCompat="@drawable/back_button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="23dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:background="@drawable/inputbutton"
        android:ems="10"
        android:hint=" 아이디"
        android:inputType="text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="98dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:background="@drawable/inputbutton"
        android:ems="10"
        android:hint=" 비밀번호"
        android:inputType="text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="165dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="96dp"
        android:layout_height="45dp"
        android:alpha="0.5"
        android:background="@android:color/transparent"
        android:text="회원가입"
        android:textSize="15sp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="321dp"
        tools:layout_editor_absoluteY="294dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>

